{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "data": {
        "url": "https://raw.githubusercontent.com/steverahardjo/viz02/refs/heads/main/src/dataset/streamgraph.csv"
    },
    "mark": "line",
        "encoding": {
        "x": {
            "timeUnit": "yearmonth",
            "field": "date",
            "axis": {
                "domain": false,
                "format": "%Y",
                "tickSize": 0
            }
        },
        "y": {
            "field": "arrivals",
            "axis": null
        },
        "color": {
            "field": "sub-region",
            "scale": {
                "scheme": "category10"
            }
        }
    }
}
