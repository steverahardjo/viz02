{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Isotype Chart representing Adjust Price per Country",
  "data": {
    "url": "https://raw.githubusercontent.com/steverahardjo/viz02/refs/heads/main/src/dataset/new_isotype.csv"
  },
  "mark": {"type": "point", "filled": true},
  "encoding": {
    "x": {"field": "col", "type": "ordinal", "axis": null}, 
    "y": {
      "field": "name",
      "type": "ordinal",
      "axis": {
        "title": "Country",
        "labelAngle": 0,
        "labelFontSize": 12
      }
    },
    "shape": {
      "value": ""
    },
    "size": {"value": 20}
  }
}

  