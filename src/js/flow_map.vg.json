{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Flow Map of Foreigners to Malaysia",
  "background": "#e0f7fa",
  "width":800,
  "height":400,
  "layer": [
    {
      "data": {
        "url": "https://cdn.jsdelivr.net/npm/vega-datasets@v1.29.0/data/world-110m.json",
        "format": {
          "type": "topojson",
          "feature": "countries"
        }
      },
      "projection": {
        "type": "equalEarth",
        "center": [400, 300]
      },
      "mark": {
        "type": "geoshape",
        "stroke": "white",
        "fill": "darkslategray"
      }
    }
  ]
}
